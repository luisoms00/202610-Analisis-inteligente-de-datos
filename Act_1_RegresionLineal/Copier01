    "#seleccionar los datos para determinar coeficicientes de relacion\n",
    "X_train = np.array(data['llamadas']).reshape(-1,1)\n",
    "y_train = np.array(data['ventas'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "db0fd87f-5d5e-41dd-a750-ea1216575387",
   "metadata": {},
   "outputs": [
